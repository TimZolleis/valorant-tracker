    "axios-cookiejar-support": "^4.0.6",
    "classnames": "^2.3.2",
    "framer-motion": "^6.5.1",
    "http-cookie-agent": "^5.0.2",
    "ioredis": "^5.2.5",
    "isbot": "^3.6.5",
    "luxon": "^3.2.1",
    "prettier": "^2.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
